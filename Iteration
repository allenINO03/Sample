public class App {
    public static void main(String[] args) throws Exception {

        for (int i = 5; i != 0; i--) {
            System.out.println("Allen");
        }
            int nicknameCount=5;    
        while (nicknameCount!=0){
        
        System.out.println ("LEN");
        nicknameCount--;
        }
        int surnameCount=5;
        do {
        System.out.println("Inocencio");
        surnameCount--;
        } while (surnameCount!=0);
        
    }
